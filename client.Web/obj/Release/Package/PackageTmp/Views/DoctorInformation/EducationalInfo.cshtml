<script>
    $(document).ready(function () {
        $('.c_site_search').hide();
        $('#EDUCATIONALQUAL').keyup(function () {
            var $th = $(this);
            $th.val($th.val().replace(/[^a-zA-Z0-9\s]*$/g, function (str) { return ''; }));
        });
        $('#PROFESSIONALMEMBERSHIP').keyup(function () {
