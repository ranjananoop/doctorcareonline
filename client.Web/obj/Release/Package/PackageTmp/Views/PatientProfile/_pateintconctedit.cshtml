        USERSINFORMATION.USERSTREETADDRESS1 = $("#USERSTREETADDRESS1").val();
        USERSINFORMATION.USERSTREETADDRESS2 = $("#USERSTREETADDRESS2").val();
        USERSINFORMATION.CITY = $("#CITY").val();
        USERSINFORMATION.USERSTATE = $("#USERSTATE").val();
        USERSINFORMATION.COUNTRY = $("#COUNTRY").val();
        USERSINFORMATION.ZIPCODE = $("#ZIPCODE").val();
        USERSINFORMATION.CurrenttimeZone = $("#CurrenttimeZone").val();

        $.ajax({
            async: false,
            type: "POST",
            cache: false,
            dataType: 'json',
            url: "/PatientProfile/_pateintconctedit",
            contentType: "application/json; charset=utf-8",
            success: function (msg) {               
                    window.location.reload();               
            },
            data: JSON.stringify(USERSINFORMATION),
        });

    });
</script>
