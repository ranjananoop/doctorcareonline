<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title </title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="Cache-Control" content="no-cache">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Expires" content="0">

    @Scripts.Render("~/bundles/modernizr")


    <!--scripts-->

    <script type="text/javascript" src="~/Scripts/jquery-1.8.3.js"></script>
    @*<script type="text/javascript" src="~/Scripts/jquery-1.9.0.min.js"></script>*@
    @*<script src="http://code.jquery.com/jquery-1.9.1.js"></script>*@
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>

    <script src="~/Scripts/imgLiquid-min.js" type="text/javascript"></script>

    <script type="text/javascript" src="~/Scripts/jquery-ui.js"></script>
    <script type="text/javascript" src="~/Scripts/jMenu.jquery.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.realperson.js"></script>
    <script type="text/javascript" src="~/Scripts/kendo.all.min.js"></script>
    <script type="text/javascript" src="~/Scripts/underscore-min.js"></script>
    <script type="text/javascript" src="~/Scripts/console.js"></script>
    <script src="~/Scripts/jquery.weekcalendar.js"></script>
    <script src="~/Scripts/jquery.password-strength.min.js"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>

    <script src="/Scripts/slider/jquery.slider.1.js" type="text/javascript"></script>
    <script src="/Scripts/slider/jquery.slider.2.js" type="text/javascript"></script>
    <script src="/Scripts/slider/jquery.slider.3.js" type="text/javascript"></script>
    <script src="/Scripts/slider/jquery.slider.4.js" type="text/javascript"></script>

    <script src="/Scripts/jquery.dropdownPlain.js" type="text/javascript"></script>


</head>
<body>
    <div >
        <div>
            <div >
                <div >
                    @RenderSection("featured", required: false)
                    <section>

                        @RenderBody()
                    </section>
                </div>
            </div>
        </div>
    </div>



    @* @Scripts.Render("~/bundles/jquery")*@
    @RenderSection("scripts", required: false)
</body>
</html>
