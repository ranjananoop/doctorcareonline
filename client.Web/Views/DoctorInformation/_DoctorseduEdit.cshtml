            <div class="editor-label">
                @Html.LabelFor(model => model.AWARDCERTIFICATION)
            </div>
            <div class="editor-field">
                @Html.TextAreaFor(model => model.AWARDCERTIFICATION)
                @Html.ValidationMessageFor(model => model.AWARDCERTIFICATION)
            </div>
        </div>
        <div class="row c_buttonContainer">
            <div class="right">
                
                <input type="button" value="Cancel" class="button cancel cancelDoctorseduEdit" />
                <input type="button" value="Save" class="button" id="doctoreduedit"/>
