            <div class="row">
                <div class="left">

                    <div class="editor-label">
                        @Html.LabelFor(model => model.PATFIRSTNAME)
                    </div>
                    <div class="editor-field">
                        @Html.TextBoxFor(model => model.PATFIRSTNAME, new { maxlength = "32" })
                        @Html.ValidationMessageFor(model => model.PATFIRSTNAME)
                    </div>
                </div>
                <div class="left">
                    <div class="editor-label">
                        @Html.LabelFor(model => model.PATLASTNAME)
                    </div>
                    <div class="editor-field">
                        @Html.TextBoxFor(model => model.PATLASTNAME, new { maxlength = "32" })
                        @Html.ValidationMessageFor(model => model.PATLASTNAME)
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="left">
                    <div class="editor-label">
                        @Html.LabelFor(model => model.GENDER)
                    </div>
                    <div class="editor-field">
                        @Html.DropDownListFor(model => model.GENDER, new[] {
                                new SelectListItem(){Text = "Male" ,Value="Male" },
                                new SelectListItem(){Text ="Female", Value ="Female" }
                            }, "Select Your Gender")
                        @Html.ValidationMessageFor(model => model.GENDER)
                    </div>
                </div>
            </div>


            <div class="row">
                <div class="left">
                    <div class="editor-label">
                        @Html.LabelFor(model => model.UserName)
                    </div>
                    <div class="editor-field">
