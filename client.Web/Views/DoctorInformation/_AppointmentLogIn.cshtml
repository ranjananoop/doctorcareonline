<script>



    $(document).ready(function (e) {

        $(":radio:eq(0)").click(function () {
            $(".frameContent").show();
            $(".login").hide();
        });

        $(":radio:eq(1)").click(function () {
            $(".frameContent").hide();
            $(".login").show();
        });



        $("#btnlogOn").click(function (e) {
            var LogOnModel = new Object();




            LogOnModel.UserName = $("#UserNameLogin").val();
            LogOnModel.Password = $("#Password").val();

            $.ajax({
                async: false,
                type: "POST",
                cache: false,
                dataType: 'json',
                url: "/Account/LogOnOuter",
                contentType: "application/json; charset=utf-8",
                success: function (msg) {
                    if (msg.msg == "success") {
                        $("#MessageUpdateStatus").val("");
                        tab("tab1");
                    } else {
                        $("#MessageUpdateStatus").val(msg.msg);
                        if ($("#MessageUpdateStatus").val().length > 0) {
                            $("#MessageUpdateStatus").show();
                        } else {
                            $("#MessageUpdateStatus").hide();
                        }
                    }


                },
                data: JSON.stringify(LogOnModel),
            });


        });


        $("#OuterRegis").click(function (e) {
            var BookingAppointmentModelOuter = new Object();




            BookingAppointmentModelOuter.PATFIRSTNAME = $("#PATFIRSTNAME").val();
            BookingAppointmentModelOuter.PATLASTNAME = $("#PATLASTNAME").val();
            BookingAppointmentModelOuter.GENDER = $("#GENDER").val();
            BookingAppointmentModelOuter.UserName = $("#UserName").val();
            BookingAppointmentModelOuter.PASSWORDS = $("#PASSWORDS").val();
            BookingAppointmentModelOuter.USERSTATE = $("#USERSTATE").val();
            BookingAppointmentModelOuter.COUNTRY = $("#COUNTRY").val();
            BookingAppointmentModelOuter.SECURITYQESTID1 = $("#SECURITYQESTID1").val();
            BookingAppointmentModelOuter.ANSWER1 = $("#ANSWER1").val();
            BookingAppointmentModelOuter.VERIFICATIONCODE = $("#VERIFICATIONCODE").val();
            BookingAppointmentModelOuter.AGREETERMS = $("#AGREETERMS").val();


            $.ajax({
                async: false,
                type: "POST",
                cache: false,
                dataType: 'json',
                url: "/Registration/LogOn",
                contentType: "application/json; charset=utf-8",
                success: function (msg) {
                    if (msg.msg == "sucess") {
                        var url = '@Url.Action("RegistrationSuccess", "Account")';
                        window.location.href = url;
                    }
                    else {
                        $("#MessageUpdateStatus").val(msg.msg);
                        if ($("#MessageUpdateStatus").val().length > 0) {
                            $("#MessageUpdateStatus").show();
                        } else {
                            $("#MessageUpdateStatus").hide();
                        }
                    }

                },
                data: JSON.stringify(BookingAppointmentModelOuter),
            });


        });
    });

</script>
